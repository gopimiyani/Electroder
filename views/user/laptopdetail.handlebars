<!--LaptopDetail Start-->
<div id="heading-breadcrumbs">
  <div class="container">
    <div class="row d-flex align-items-center flex-wrap">
      <div class="col-md-7">
        <h1 class="h2">Laptop Detail</h1>
      </div>

    </div>
  </div>
</div>
<br>
<div id="content">
  <div class="container">
    <div id="productMain" class="row">
      <div class="col-sm-6">
        <div class="box">
        <div data-slider-id="1" class="owl-carousel shop-detail-carousel">
          <div> <img src="{{laptopDetail.prod_image1}}" alt="laptopimage" class="img-fluid"></div>
          <div> <img src="{{laptopDetail.prod_image2}}" alt="laptopimage" class="img-fluid"></div>
          <div> <img src="{{laptopDetail.prod_image3}}" alt="laptopimage" class="img-fluid"></div>
        </div>
        </div>
      </div>
      <div class="col-sm-6">
        <div class="box">
          <form>
              <p class="price">{{laptopDetail.prod_name}}</p>
            <p class="price">${{laptopDetail.prod_price}}</p>
            <p class="text-center">
              {{!-- <button type="submit" class="btn btn-template-outlined">
                <i class="fa fa-shopping-cart"></i>
                Add to cart</button> --}}
              {{!-- <button type="submit" data-toggle="tooltip" data-placement="top" title="Add to wishlist" class="btn btn-default"><i
                  class="fa fa-heart-o"></i></button> --}}
            </p>
          </form>
        </div>
        <div data-slider-id="1" class="owl-thumbs">
          <button class="owl-thumb-item"><img src="{{laptopDetail.prod_image1}}" alt="laptopimage" class="img-fluid"></button>
          <button class="owl-thumb-item"><img src="{{laptopDetail.prod_image2}}" alt="laptopimage" class="img-fluid"></button>
          <button class="owl-thumb-item"><img src="{{laptopDetail.prod_image3}}" alt="laptopimage" class="img-fluid"></button>
        </div>
      </div>
    </div>
    <div id="details" class="box mb-4 mt-4">
      <p></p>
      <h3>{{laptopDetail.prod_name}}</h3>
      <h4>Description</h4>
      
      <ul>
        <li>Brand:{{laptopDetail.prod_brand}} </li>
        <li>Price: ${{laptopDetail.prod_price}}</li>
        <li>Screen Size: {{laptopDetail.prod_screensize}}</li>
        <li>RAM:{{laptopDetail.prod_ram}}</li>
        <li>Processor: {{laptopDetail.prod_processor}}</li>
        <li>Hard Disk Size: {{laptopDetail.prod_hard_disk_size}}</li>
        <li>Rating: {{laptopDetail.prod_rating}}</li>
      </ul>
    </div>
  </div>
</div>

<!--LaptopDetail End-->